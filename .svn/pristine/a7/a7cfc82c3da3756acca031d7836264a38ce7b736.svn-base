package stream.api.adapter.com.unibank;

/**
 *
 * @author Azer F. Mehdiyev (Azar.mehdiyev@unibank.az)
 */
public class Detail implements stream.api.Detail{

    private String name;
    private String value;

    public Detail(String name, String value) {
        this.name = name;
        this.value = value;
    }

    public Detail() {
    }
    
    @Override
    public String getName() {
        return name;
    }

    @Override
    public String getValue() {
        return value;
    }

}
